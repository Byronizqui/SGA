<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.byron.sga.Profesores">

    <!-- TODO: Update blank fragment layout -->

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp">

        <Button
            android:id="@+id/button10"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="83dp"
            android:layout_height="48dp"
            android:backgroundTint="@color/colorPrimary"
            android:elevation="0dp"
            android:text="Editar un profesor"
            android:textSize="8sp"
            app:layout_constraintLeft_toRightOf="@+id/button13"
            app:layout_constraintTop_toTopOf="@+id/button13"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1" />

        <Button
            android:id="@+id/button11"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="83dp"
            android:layout_height="48dp"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="16dp"
            android:backgroundTint="@color/colorPrimary"
            android:text="Busqueda por nombre"
            android:textSize="8sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/button13"
            app:layout_constraintLeft_toLeftOf="parent"
            tools:layout_constraintBaseline_creator="1"
            tools:layout_constraintLeft_creator="1" />

        <Button
            android:id="@+id/button12"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="83dp"
            android:layout_height="48dp"
            android:backgroundTint="@color/colorPrimary"
            android:text="Agregar un profesor"
            android:textSize="8sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/button10"
            app:layout_constraintLeft_toRightOf="@+id/button10"
            tools:layout_constraintBaseline_creator="1"
            tools:layout_constraintLeft_creator="1" />

        <Button
            android:id="@+id/button13"
            style="@style/Widget.AppCompat.Button.Colored"
            android:layout_width="83dp"
            android:layout_height="48dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="8dp"
            android:backgroundTint="@color/colorPrimary"
            android:text="Busqueda por cedula"
            android:textSize="8sp"
            app:layout_constraintLeft_toRightOf="@+id/button11"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1" />
    </android.support.constraint.ConstraintLayout>
</FrameLayout>
